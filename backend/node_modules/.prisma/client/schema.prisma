// backend/prisma/schema.prisma
// Prisma schema for Udyam form submissions

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UdyamSubmission {
  id           Int      @id @default(autoincrement())
  aadhaar      String
  aadhaar_name String
  otp          String
  pan          String
  createdAt    DateTime @default(now())
}
